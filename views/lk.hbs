<h2>
  Личный кабинет пользователя
</h2>

<br/>

<div class="greetingCard">
  {{!-- <img class="userPhoto" src="https://upload.wikimedia.org/wikipedia/commons/thumb/1/12/User_icon_2.svg/2048px-User_icon_2.svg.png" class="card-img-top" alt="..."> --}}
  <div class="greetingText">
    <h3 class="card-body">Привет, {{username}} </h3>
    <h3 class="card-body">Пользователь с нами c {{data}} </h3>
    <a class="nav-link" href="/lkEdit/{{id}}"><button class="btn btn-primary">Редактировать</button></a>
  </div>
</div>

<br/>
<br/>

<div class="allOrders">

    <div class="card currentOrder" style="width: 18rem;">
          <div class="card-header text-center">
            Текущий заказ
          </div>
        {{#each products}}
          <div class="card-body">
              <div>
                <img class="dragPhoto" src="data:image/png;base64," class="card-img-top" alt="...">
              </div>
              <div>
                <h5 class="card-title text-left"> {{title}} </h5>
                <p class="card-text text-left">{{price}} рублей</p>
              </div>
              {{!-- <div> --}}
                {{!-- <button class="btn btn-primary">Добавить в корзину</button> --}}
              {{!-- </div> --}}
          </div>
        {{/each}} 
  </div>
    
  <div class="card prevOrder" style="width: 18rem;">
          <div class="card-header text-center">
            Предыдущий заказ
          </div>
        {{#each products}}
          <div class="card-body">
              <div>
                <img class="dragPhoto" src="data:image/png;base64," class="card-img-top" alt="...">
              </div>
              <div>
                <h5 class="card-title text-left"> {{title}} </h5>
                <p class="card-text text-left">{{price}} рублей</p>
              </div>
              {{!-- <div>
                <button class="btn btn-primary">Добавить в корзину</button>
              </div> --}}
          </div>
        {{/each}} 
          {{!-- <button class="btn btn-primary">Повторить заказ</button> --}}
  </div>
      
  
</div>

  
